meta {
  name: Get all orders for authenticated seller
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/seller/orders/all?page=1
  body: none
  auth: bearer
}

params:query {
  page: 1
}

headers {
  Accept: application/json
}

auth:bearer {
  token: {{bearerToken}}
}

settings {
  encodeUrl: true
}

docs {
  Retrieves paginated list of all orders for the authenticated seller, ordered by most recent first.
}

