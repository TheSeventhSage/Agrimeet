meta {
  name: Get seller's products
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/seller/products?page=1
  body: none
  auth: bearer
}

params:query {
  page: 1
}

headers {
  Accept: application/json
}

auth:bearer {
  token: {{bearerToken}}
}

settings {
  encodeUrl: true
}

docs {
  Retrieve a paginated list of products belonging to the authenticated seller
}

